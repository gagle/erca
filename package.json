{
  "name": "erca",
  "version": "0.0.0",
  "description": "Proceso ERCA",
  "author": "Gabriel Llamas",
  "repository": "git://github.com/gagle/erca.git",
  "main": "build/electron/main.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "concurrently \"rimraf release\" \"rimraf build\" \"rimraf coverage\" \"rimraf npm-debug.log*\" \"npm cache verify\" \"npm prune\" \"rimraf Chrome*\"",
    "build:electron": "rimraf build/electron && concurrently \"tsc -p tsconfig-electron.json\" \"copyfiles -u 3 src/electron/assets/**/* build/electron/assets\"",
    "build:ng:dev": "ng build -c development",
    "build:ng:prod": "ng build -c production",
    "build:dev": "run-p build:ng:dev build:electron",
    "build:prod": "run-p build:ng:prod build:electron",
    "serve:dev": "run-p serve:ng:dev serve:electron",
    "serve:electron": "wait-on http-get://localhost:4200/ && npm run build:electron && cross-env NODE_ENV=development electron .",
    "serve:ng:dev": "ng serve -c development",
    "postinstall": "node scripts/postinstall && electron-builder install-app-deps",
    "release": "npm run build:prod && electron-builder build --windows --ia32",
    "version:bump": "gulp version",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "ng test --source-map=false --browsers=Chrome",
    "test:coverage": "ng test --code-coverage --browsers=Chrome",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "7.0.4",
    "@angular/cdk": "7.0.4",
    "@angular/common": "7.0.4",
    "@angular/compiler": "7.0.4",
    "@angular/core": "7.0.4",
    "@angular/forms": "7.0.4",
    "@angular/http": "7.0.4",
    "@angular/material": "7.0.4",
    "@angular/platform-browser": "7.0.4",
    "@angular/platform-browser-dynamic": "7.0.4",
    "@angular/router": "7.0.4",
    "core-js": "2.5.7",
    "d3": "5.7.0",
    "fs-extra": "7.0.1",
    "immutable": "4.0.0-rc.12",
    "nedb": "1.8.0",
    "ngx-infinite-scroll": "6.0.1",
    "redis-parser": "3.0.0",
    "rxjs": "6.3.3",
    "uuid": "3.3.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.6",
    "@angular/cli": "7.0.6",
    "@angular/compiler-cli": "7.0.4",
    "@angular/language-service": "7.0.4",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-angular": "7.1.2",
    "@types/d3": "5.0.1",
    "@types/fs-extra": "^5.0.4",
    "@types/jasmine": "3.0.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.9",
    "@types/uuid": "3.4.4",
    "codelyzer": "4.5.0",
    "concurrently": "4.0.1",
    "conventional-changelog-cli": "2.0.11",
    "conventional-recommended-bump": "4.0.4",
    "copyfiles": "2.1.0",
    "cross-env": "5.2.0",
    "cucumber": "5.0.2",
    "electron": "3.0.9",
    "electron-builder": "20.36.2",
    "electron-reload": "1.3.0",
    "gulp": "3.9.1",
    "husky": "1.1.4",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "lint-staged": "8.0.5",
    "npm-run-all": "4.1.3",
    "prettier": "1.15.2",
    "protractor": "5.4.1",
    "rimraf": "2.6.2",
    "rxjs-tslint": "0.1.5",
    "semver": "5.6.0",
    "spectron": "5.0.0",
    "stylelint": "9.8.0",
    "stylelint-config-prettier": "4.0.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "1.0.0",
    "stylelint-scss": "3.4.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.16.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.1.6",
    "wait-on": "3.2.0",
    "webdriver-manager": "12.1.0"
  }
}
